@model IEnumerable<AspNetCoreApp.Components.AdminMenuItem>

<li class="dropdown">
    <a asp-controller="Admin"
       asp-action="Index"
       class="dropdown-toggle" data-toggle="dropdown">Admin<b class="caret"></b></a>
    <ul class="dropdown-menu">
        <li class="dropdown">
            <a asp-controller="PieManagement"
               asp-action="Index">Pie Management</a>
        </li>
        @foreach (var menuItem in Model)
        {
            <li>
                <a asp-controller="Admin" asp-action=@menuItem.ActionValue>@menuItem.DisplayValue</a>
            </li>
        }
    </ul>
</li>